package com.innouni.yinongbao.unit.exhibition;

import java.util.List;

/***
 * 农资展厅分类父级实体类
 * 
 * @author LinYuLing
 * @UpdateDate 2014-09-27
 */
public class ExhibitionTypeUnit {
	/***
	 * 父级id
	 */
	private String catId;
	/***
	 * 父级名称
	 */
	private String catName;
	/***
	 * 子级数据列表
	 */
	private List<ExhibitionTypeCUnit> c_types;

	public String getCatId() {
		return catId;
	}

	public void setCatId(String catId) {
		this.catId = catId;
	}

	public String getCatName() {
		return catName;
	}

	public void setCatName(String catName) {
		this.catName = catName;
	}

	public List<ExhibitionTypeCUnit> getC_types() {
		return c_types;
	}

	public void setC_types(List<ExhibitionTypeCUnit> c_types) {
		this.c_types = c_types;
	}
}
